<?xml version="1.0" encoding="UTF-8"?>
<executable>
  <category>BigImage</category>
  <title>NucleusSegmentation</title>
  <description><![CDATA[This is a CLI module that can be bundled in an extension]]></description>
  <version>0.0.1</version>
  <documentation-url>https://slicerscope.github.io</documentation-url>
  <license>Slicer</license>
  <contributor>Yi Gao (Shenzhen University)</contributor>
  <acknowledgements></acknowledgements>
  <parameters>
    <label>IO</label>
    <description><![CDATA[Input/output parameters]]></description>
    <double>
      <name>threshold</name>
      <longflag>threshold</longflag>
      <flag>t</flag>
      <label>threshold</label>
      <description><![CDATA[A double value (in units of mm) passed to the algorithm]]></description>
      <default>75.0</default>
    </double>
    <double>
      <name>mpp</name>
      <longflag>mpp</longflag>
      <flag>m</flag>
      <label>mpp</label>
      <description><![CDATA[micron per pixel]]></description>
      <default>0.25</default>
    </double>
    <image type="vector">
      <name>inputfileName</name>
      <label>Input HE RGB Image</label>
      <channel>input</channel>
      <index>0</index>
      <description><![CDATA[Input image]]></description>
    </image>
